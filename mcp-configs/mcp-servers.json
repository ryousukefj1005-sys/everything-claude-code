{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_PAT_HERE"
      },
      "description": "GitHub operations - PRs, issues, repos"
    },
    "firecrawl": {
      "command": "npx",
      "args": ["-y", "firecrawl-mcp"],
      "env": {
        "FIRECRAWL_API_KEY": "YOUR_FIRECRAWL_KEY_HERE"
      },
      "description": "Web scraping and crawling"
    },
    "supabase": {
      "command": "npx",
      "args": ["-y", "@supabase/mcp-server-supabase@latest", "--project-ref=YOUR_PROJECT_REF"],
      "description": "Supabase database operations"
    },
    "memory": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "description": "Persistent memory across sessions"
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "description": "Chain-of-thought reasoning"
    },
    "vercel": {
      "type": "http",
      "url": "https://mcp.vercel.com",
      "description": "Vercel deployments and projects"
    },
    "railway": {
      "command": "npx",
      "args": ["-y", "@railway/mcp-server"],
      "description": "Railway deployments"
    },
    "cloudflare-docs": {
      "type": "http",
      "url": "https://docs.mcp.cloudflare.com/mcp",
      "description": "Cloudflare documentation search"
    },
    "cloudflare-workers-builds": {
      "type": "http",
      "url": "https://builds.mcp.cloudflare.com/mcp",
      "description": "Cloudflare Workers builds"
    },
    "cloudflare-workers-bindings": {
      "type": "http",
      "url": "https://bindings.mcp.cloudflare.com/mcp",
      "description": "Cloudflare Workers bindings"
    },
    "cloudflare-observability": {
      "type": "http",
      "url": "https://observability.mcp.cloudflare.com/mcp",
      "description": "Cloudflare observability/logs"
    },
    "clickhouse": {
      "type": "http",
      "url": "https://mcp.clickhouse.cloud/mcp",
      "description": "ClickHouse analytics queries"
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@context7/mcp-server"],
      "description": "Live documentation lookup"
    },
    "magic": {
      "command": "npx",
      "args": ["-y", "@magicuidesign/mcp@latest"],
      "description": "Magic UI components"
    },
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/path/to/your/projects"],
      "description": "Filesystem operations (set your path)"
    },
    "_z_ai_mcp_servers": {
      "zai-mcp-server": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@z_ai/mcp-server"],
        "env": {
          "Z_AI_API_KEY": "YOUR_Z_AI_API_KEY",
          "Z_AI_MODE": "ZAI"
        },
        "description": "Z.AI Vision - 图片/视频分析、OCR、UI转代码 (8个工具)"
      },
      "zai-brave-search": {
        "type": "http",
        "url": "https://api.z.ai/api/mcp/search/mcp",
        "headers": {
          "Authorization": "Bearer YOUR_Z_AI_API_KEY"
        },
        "description": "Z.AI Search - 联网搜索 (Brave)"
      },
      "zai-web-reader": {
        "type": "streamable-http",
        "url": "https://api.z.ai/api/mcp/web_reader/mcp",
        "headers": {
          "Authorization": "Bearer YOUR_Z_AI_API_KEY"
        },
        "description": "Z.AI Reader - 网页内容读取"
      },
      "zai-zread": {
        "type": "streamable-http",
        "url": "https://api.z.ai/api/mcp/zread/mcp",
        "headers": {
          "Authorization": "Bearer YOUR_Z_AI_API_KEY"
        },
        "description": "Z.AI Zread - GitHub仓库文档搜索"
      }
    }
  },
  "_comments": {
    "z_ai_note": "Z.AI MCP 服务器 - 需要从 https://open.bigmodel.cn/ 获取 API Key",
    "zai-mcp-server": "提供 8 个工具: ui_to_artifact, extract_text_from_screenshot, diagnose_error_screenshot, understand_technical_diagram, analyze_data_visualization, ui_diff_check, image_analysis, video_analysis",
    "usage": "Copy the servers you need to your ~/.claude.json mcpServers section",
    "env_vars": "Replace YOUR_*_HERE placeholders with actual values",
    "disabling": "Use disabledMcpServers array in project config to disable per-project",
    "context_warning": "Keep under 10 MCPs enabled to preserve context window"
  }
}
